{
  "name": "stencil-component-lib",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "module": "dist/esm/index.js",
  "main": "dist/index.js",
  "unpkg": "dist/ionic.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/",
    "preview/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve --docs",
    "build:watch": "stencil build --watch",
    "unit-test": "stencil test --spec",
    "unit-test:watch": "stencil test --spec --watchAll",
    "e2e-test": "stencil test --e2e",
    "e2e-test:watch": "stencil test --e2e --watchAll",
    "test": "stencil test --spec --e2e",
    "test:watch": "stencil test --spec --e2e --watchAll"
  },
  "dependencies": {},
  "devDependencies": {
    "@stencil/core": "~0.16.2",
    "@stencil/sass": "^0.1.1",
    "@types/jest": "^23.3.13",
    "@types/puppeteer": "1.11.2",
    "jest": "23.6.0",
    "jest-cli": "23.6.0",
    "puppeteer": "1.8.0"
  },
  "license": "MIT"
}
